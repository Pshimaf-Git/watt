import 'std.io'
import 'std.math'

io.println('sin(1): ' + math.sin(1))
io.println('cos(1): ' + math.cos(1))
io.println('asin(1): ' + math.asin(1))
io.println('acos(1): ' + math.acos(1))
io.println('tan(1): ' + math.tan(1))
io.println('atan(1): ' + math.atan(1))
io.println('ctg(1): ' + math.ctg(1))
io.println('sqrt(100): ' + math.sqrt(100))
io.println('cbrt(27): ' + math.cbrt(27))
io.println('root(16, 4): ' + math.root(16, 4))
io.println('log(16): ' + math.log(16))
io.println('log10(100): ' + math.log10(100))
io.println('pow(10, 3): ' + math.pow(10, 3))
io.println('exp(10): ' + math.exp(10))
io.println('round(3.5): ' + math.round(3.5))
io.println('floor(3.6): ' + math.floor(3.6))
io.println('ceil(3.4)): ' + math.ceil(3.4))
io.println('abs(-3): ' + math.abs(-3))
io.println('mean(10,10,20): ' + math.mean([10,10,20]))
io.println('hmean(10,10,20): ' + math.hmean([10,10,20]))
io.println('median: ' + math.median([10,10,20,35]))
io.println('median 2: ' + math.median([10,35,20,40,70]))
io.println('mode: ' + math.mode([10,20,10,10,3,5,5,5,53,7]))
io.println('popularity: ' + math.popularity([10,20,10,10,3,5,5,5,53,7]).to_string())
